{
  "name": "lab-gui",
  "version": "1.0.2",
  "description": "UI Framework for controling a lab",
  "keywords": [
    "react",
    "Ui",
    "typescript"
  ],
  "main": "dist/lab-gui.umd.js",
  "module": "dist/lab-gui.es5.js",
  "typings": "dist/types/lab-gui.d.ts",
  "files": [
    "dist"
  ],
  "author": "Sebastian Weigand <s.weigand.phy@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MicroControlLab/lab-gui"
  },
  "bugs": {
    "url": "https://github.com/MicroControlLab/lab-gui-websocket.git/issues"
  },
  "homepage": "https://github.com/MicroControlLab/lab-gui-websocket.git",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "tslint -t codeFrame -c tslint.json 'src/**/*.{ts,tsx,js}' 'tests/**/*.{ts,tsx,js}'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "demo": "parcel demos/demo.html"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts,tsx,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ],
    "collectCoverageFrom": [
      "src/*.{js,ts,tsx}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^13.1.0",
    "@types/parcel-env": "0.0.0",
    "@types/plotly.js": "^1.38.12",
    "@types/rc-slider": "^8.6.0",
    "@types/react": "^16.8.25",
    "@types/react-dom": "^16.0.7",
    "@types/react-plotly.js": "^2.2.2",
    "@types/react-redux": "^7.0.0",
    "@types/redux-logger": "^3.0.6",
    "colors": "^1.3.2",
    "commitizen": "^4.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^7.0.0",
    "cz-conventional-changelog": "^3.0.0",
    "husky": "^4.0.2",
    "i": "^0.3.6",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "lint-staged": "^10.0.3",
    "lodash.camelcase": "^4.3.0",
    "npm": "^6.10.2",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "redux-devtools-extension": "^2.13.5",
    "replace-in-file": "^5.0.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.19.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.25.1",
    "semantic-release": "^17.0.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-react": "^4.0.0",
    "typedoc": "^0.16.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "plotly.js": "^1.49.0",
    "rc-slider": "^8.6.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-plotly.js": "^2.3.0",
    "react-redux": "^7.0.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "lab-gui-websocket": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
